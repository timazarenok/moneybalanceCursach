<Window x:Class="Money.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Money"
        mc:Ignorable="d"
        Title="AdminWindow" Height="350" Width="400">
    <Grid>
        <Button x:Name="Category" Content="Добавление категории" Width="200" Height="30" Click="Category_Click"/>
        <Button x:Name="Exit" Width="100" Height="25" Content="Выход" Margin="0,270,250,0" Click="Exit_Click" />
    </Grid>
</Window>
